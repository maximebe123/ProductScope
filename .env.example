# Frontend Environment Variables
# Copy this file to .env and fill in your values

# Backend API URL (default: http://localhost:8000)
VITE_API_URL=http://localhost:8000

# WebSocket URL for diagrams realtime (default: ws://localhost:8000/api/diagrams/ws/realtime)
VITE_WS_URL=ws://localhost:8000/api/diagrams/ws/realtime

# WebSocket URL for mindmap realtime (default: ws://localhost:8000/api/mindmap/ws/realtime)
VITE_WS_MINDMAP_URL=ws://localhost:8000/api/mindmap/ws/realtime
